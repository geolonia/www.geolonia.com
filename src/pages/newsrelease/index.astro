---
import MarkdownPageLayout from '../../layouts/MarkdownPageLayout.astro';
import { getEntry } from 'astro:content';

const entry = await getEntry('pages', 'newsrelease');
const { Content } = await entry.render();
---

<MarkdownPageLayout title={entry.data.title} description={entry.data.description} lead="News Release">
  <Content />
</MarkdownPageLayout>
