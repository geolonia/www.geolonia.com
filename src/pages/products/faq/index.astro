---
import MarkdownPageLayout from '../../../layouts/MarkdownPageLayout.astro';
import { getEntry } from 'astro:content';

const entry = await getEntry('pages', 'products/faq');
const { Content } = await entry.render();
---

<MarkdownPageLayout title={entry.data.title} description={entry.data.description} lead="FAQ">
  <Content />
</MarkdownPageLayout>
